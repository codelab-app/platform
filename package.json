  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "e2e": "nx e2e-ui web-e2e -c test",
    "e2e:web": "nx serve web -c test",
    "e2e:api": "nx serve api -c test",
    "cli": "scripts/nx/cli.sh",
    "docker:up": "scripts/docker/dev/up.sh",
    "docker:prod:up": "scripts/docker/prod/up.sh",
    "codegen": "pnpm cli tasks graphql-codegen --stage dev",
    "build": "nx run-many --all --target=build",
    "test": "nx run-many --all --target=test --parallel=1 --runInBand",
    "test:unit": "nx run-many --all --target=test:unit --parallel=5",
    "test:integration": "nx run-many --all --target=test:integration --parallel=1",
    "lint": "nx run-many --all --target=lint --parallel=5",
    "format": "npx prettier --write \"./**/*.{graphql,yaml,json}\" && terraform -chdir=infra/terraform fmt --recursive",
    "nx": "nx",
    "update": "nx migrate @nx/workspace",
    "dep-graph": "nx dep-graph --watch",
    "help": "nx help",
    "commit": "git-cz",
    "cpack": "scripts/circleci/circleci-pack.sh",
    "git:prune": "git gc --aggressive --prune=now",
    "prepare": "husky",
    "knip": "knip"
  },
