{
  "name": "codelab",
  "engines": {
    "node": ">=22.14.0",
    "yarn": "please-use-pnpm",
    "npm": "please-use-pnpm",
    "pnpm": ">=9.0.0"
  },
  "version": "0.0.1",
  "license": "MIT",
  "sideEffects": false,
  "msw": {
    "workerDirectory": [
      "apps/web/public"
    ]
  },
  "scripts": {
    "serve": "NX_TUI=true nx run-many --target=serve --projects=api,web --parallel",
    "serve:sites": "NX_TUI=true nx run-many --target=serve --projects=api,web,sites --parallel",
    "serve:test": "NX_TUI=false nx run-many --target=serve --projects=api,web --parallel -c test",
    "serve:sites:test": "NX_TUI=false nx run-many --target=serve --projects=api,web,sites --parallel -c test",
    "caddy": "caddy run --config scripts/caddy/Caddyfile",
    "e2e": "nx e2e-ui web-e2e -c test",
    "e2e:web": "nx serve web -c test",
    "e2e:api": "nx serve api -c test",
    "cli": "scripts/nx/cli.sh",
    "docker:up": "scripts/docker/dev/up.sh",
    "docker:prod:up": "scripts/docker/prod/up.sh",
    "codegen": "pnpm cli tasks graphql-codegen --stage dev",
    "build": "nx run-many --all --target=build",
    "test": "nx run-many --all --target=test --parallel=1 --runInBand",
    "test.unit": "nx run-many --all --target=test -c test.unit --parallel=5",
    "test.integration": "nx run-many --all --target=test -c test.integration --parallel=1",
    "lint": "nx run-many --all --target=lint --parallel=5",
    "format": "npx prettier --write \"./**/*.{graphql,yaml,json,css}\" && terraform -chdir=infra/terraform fmt --recursive",
    "update": "nx migrate @nx/workspace",
    "dep-graph": "nx dep-graph --watch",
    "help": "nx help",
    "commit": "git-cz",
    "cpack": "scripts/circleci/circleci-pack.sh",
    "git:prune": "git gc --aggressive --prune=now",
    "prepare": "husky",
    "knip": "knip",
    "aider": "node scripts/aider/run.js",
    "nx:clean-cache": "rimraf .nx/cache && mkdir -p .nx/cache",
    "browser-tools": "npx @agentdeskai/browser-tools-server@latest"
  },
  "private": true,
  "validate-branch-name": {
    "pattern": "^(feat|fix|refactor|test)\\/[0-9]+-[a-z0-9-]+$|^(master|main|dev|staging|nx-upgrade)$|^(stag|prod)-(0|[1-9][0-9]*)\\.(0|[1-9][0-9]*)\\.(0|[1-9][0-9]*)$|^release\\/(staging|prod)$",
    "errorMsg": "Branch name must follow pattern: <type>/<issue-number>-<description>\n\nAllowed types: feat, fix, refactor, test\nExample: feat/3736-debug-env-service-tracking\n\nAlso allowed: master, main, dev, staging, nx-upgrade, release/staging, release/prod, or release tags (stag-x.x.x, prod-x.x.x)"
  },
