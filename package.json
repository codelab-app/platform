  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "setup": "./scripts/yarn/setup.sh",
    "postinstall": "./scripts/yarn/postinstall.sh && husky install",
    "serve:test": "npx env-cmd -f .env.test nx serve web -c test",
    "e2e": "env-cmd -f .env.test nx e2e builder-e2e -c dev",
    "cli": "node dist/apps/cli/main.js",
    "docker:up": "scripts/docker/up.sh",
    "tunnel": "ngrok http --region=us --hostname=api.codelab-dev.ngrok.io 127.0.0.1:3000",
    "codegen": "yarn cli tasks codegen --stage dev",
    "build": "nx run-many --all --target=build",
    "build:test": "npx env-cmd -f .env.test nx build builder -c test",
    "test": "nx run-many --all --target=test --parallel=3",
    "lint": "nx workspace-lint && nx run-many --all --target=lint --parallel=5",
    "nx": "nx",
    "update": "nx migrate @nrwl/workspace",
    "dep-graph": "nx dep-graph --watch",
    "help": "nx help",
    "commit": "git-cz",
    "git:prune": "git gc --aggressive --prune=now"
  },
