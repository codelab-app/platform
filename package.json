  "version": "0.0.1",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "postinstall": "nx run-many build --target=tools-workspace,eslint-plugin",
    "e2e": "yarn nx e2e platform-e2e",
    "cli": "scripts/nx/cli.sh",
    "docker:up": "scripts/docker/up.sh",
    "tunnel": "ngrok http --region=us --hostname=api.codelab-dev.ngrok.io 127.0.0.1:3000",
    "codegen": "yarn cli tasks codegen --stage dev",
    "build": "nx run-many --all --target=build",
    "test": "nx run-many --all --target=test --parallel=5",
    "test:unit": "nx run-many --all --target=test:unit --parallel=5",
    "test:integration": "nx run-many --all --target=test:integration --parallel=5",
    "lint": "nx run-many --all --target=lint --parallel=5 --quiet",
    "format": "npx prettier --write \"./**/*.{graphql,yaml,json}\" && terraform -chdir=terraform fmt --recursive",
    "nx": "nx",
    "update": "nx migrate @nx/workspace",
    "dep-graph": "nx dep-graph --watch",
    "help": "nx help",
    "commit": "git-cz",
    "cpack": "scripts/circleci/circleci-pack.sh",
    "git:prune": "git gc --aggressive --prune=now",
    "project:update": "nx build tools-workspace && yarn && nx generate @codelab/workspace:nx-project-config"
  },
