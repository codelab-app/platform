# Shared variables for all Packer builds
# This file contains common configuration values used across different Packer templates

# DigitalOcean region for all deployments
# sgp1 = Singapore
region = "sgp1"

# Docker Compose version
docker_compose_version = "2.24.0"

# DigitalOcean CLI version
doctl_version = "1.104.0"

# Consul version
consul_version = "1.17.2"