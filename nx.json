{
  "defaultBase": "master",
  "maxCacheSize": "8GB",
  "nxCloudId": "67fcdb6b7eeb2ddcc348061b",
  "sync": {
    "applyChanges": true
  },
  "plugins": [
    {
      "plugin": "@nx/js/typescript",
      "options": {
        "typecheck": {
          "targetName": "typecheck"
        },
        "build": {
          "targetName": "build",
          "configName": "tsconfig.lib.json",
          "buildDepsName": "build-deps",
          "watchDepsName": "watch-deps"
        }
      }
    },
    {
      "plugin": "@nx/eslint/plugin",
      "options": {
        "targetName": "lint"
        // "extensions": ["ts", "tsx", "js", "jsx", "graphql"]
      }
    },
    {
      "plugin": "@nx/next/plugin",
      "options": {
        "startTargetName": "start",
        "buildTargetName": "build",
        "devTargetName": "dev",
        "serveStaticTargetName": "serve-static",
        "buildDepsTargetName": "build-deps",
        "watchDepsTargetName": "watch-deps"
      }
    },
    {
      "plugin": "@nx/jest/plugin",
      "options": {
        "targetName": "test"
      }
    },
    {
      "plugin": "@nx/webpack/plugin",
      "options": {
        "buildTargetName": "build",
        "serveTargetName": "serve",
        "previewTargetName": "preview",
        "buildDepsTargetName": "build-deps",
        "watchDepsTargetName": "watch-deps"
      }
    },
    {
      "plugin": "@nx/vite/plugin",
      "options": {
        "buildTargetName": "build",
        "previewTargetName": "preview",
        "buildDepsTargetName": "build-deps",
        "testTargetName": "test",
        "serveTargetName": "serve",
        "serveStaticTargetName": "serve-static"
      }
    }
    // Last one takes precedence
    // {
    //   "plugin": "@codelab/tools-workspace/plugin/react"
    // }
  ],
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"]
    },
    "typecheck": {
      "dependsOn": ["^typecheck"]
    },
    // "@nx/vite:build": {
    //   "cache": true,
    //   "outputs": ["{options.outputPath}"],
    //   "dependsOn": ["^build"]
    // },
    "@nx/js:tsc": {
      "options": {
        "main": "{projectRoot}/src/index.ts",
        "outputPath": "{projectRoot}/dist",
        "tsConfig": "{projectRoot}/tsconfig.lib.json",
        "assets": [
          "{projectRoot}/**/files/*__tmpl__",
          "{projectRoot}/executors.json",
          "{projectRoot}/generators.json"
        ]
      }
    },
    "@nx/vite:build": {
      "cache": true,
      "dependsOn": [],
      "options": {
        "buildLibsFromSource": false
      }
    },
    "@nx/next:build": {
      "cache": true,
      "options": {
        "postcssConfig": "apps/web/postcss.config.js",
        "buildLibsFromSource": false
      },
      "configurations": {
        "dev": {
          "buildLibsFromSource": true
        },
        "production": {}
      },
      "dependsOn": ["^build"]
    },
    // "tsc-check": {
    //   "cache": true,
    //   "inputs": ["default", "^default", "{workspaceRoot}/tsconfig.base.json"]
    // },
    "@nx/jest:jest": {
      "cache": true,
      "defaultConfiguration": "test",
      "inputs": ["default", "^default", "{workspaceRoot}/jest.preset.ts"],
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "parallel": 1,
        "passWithNoTests": true,
        "jestConfig": "{projectRoot}/jest.config.ts",
        "runInBand": true
      },
      "configurations": {
        "test": {
          "reporters": ["default"]
        },
        "test.unit": {
          "reporters": ["default"],
          "color": true,
          "parallel": 3,
          "testPathIgnorePatterns": ["i.spec.ts"]
        },
        "ci.unit": {
          "ci": true,
          "codeCoverage": true,
          "color": true,
          "parallel": 3,
          "testPathIgnorePatterns": ["i.spec.ts"]
        },
        "test.integration": {
          "reporters": ["default"],
          "color": true,
          "testPathPattern": ["i.spec.ts"],
          "runInBand": true
        },
        "ci.integration": {
          "ci": true,
          "codeCoverage": false,
          "color": true,
          "testPathPattern": ["i.spec.ts"],
          "runInBand": true
        }
      }
    },
    "@nx/eslint:lint": {
      "cache": true,
      "inputs": [
        "default",
        "{workspaceRoot}/.eslintrc.json",
        "{projectRoot}/.eslintrc.json"
      ],
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["{projectRoot}/**/*.{ts,tsx,js,jsx}"]
      },
      "configurations": {
        "ci": {
          "format": "junit",
          "outputFile": "tmp/reports/lint/{projectName}.xml",
          "quiet": true
        },
        "codegen": {
          "lintFilePatterns": ["{projectRoot}/**/*.gen.{ts,tsx}"],
          "errorOnUnmatchedPattern": false
        }
      }
    }
  },
  "generators": {
    "@nx/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nx/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nx/react": {
      "application": {
        "style": "css",
        "linter": "eslint"
      },
      "library": {
        "style": "styled-components",
        "linter": "eslint",
        "unitTestRunner": "none",
        "compiler": "swc",
        "component": false,
        "name": "",
        "directory": "libs/frontend/",
        "setParserOptionsProject": true,
        "strict": true,
        "importPath": "@codelab/frontend/",
        "tags": "scope:frontend"
      },
      "storybook-configuration": {
        "linter": "eslint"
      },
      "component": {
        "style": "css"
      }
    },
    "@nx/plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nx/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint",
        "setParserOptionsProject": true,
        "strict": true,
        "tags": "scope:backend"
      }
    },
    "@nx/next": {
      "application": {
        "style": "styled-components",
        "linter": "eslint"
      }
    },
    "@nx/js": {
      "library": {
        "unitTestRunner": "none",
        "bundler": "none",
        "setParserOptionsProject": true,
        "linter": "eslint",
        "strict": true,
        "importPath": "@codelab/frontend",
        "tags": "scope:shared",
        "useProjectJson": true,
        "skipPackageJson": true
      }
    }
  },
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "namedInputs": {
    "default": ["{projectRoot}/**/*", "sharedGlobals"],
    "sharedGlobals": [
      "{workspaceRoot}/workspace.json",
      "{workspaceRoot}/tslint.json",
      "{workspaceRoot}/nx.json",
      "{workspaceRoot}/tsconfig.base.json"
    ],
    "prod": [
      "default",
      "!{projectRoot}/**/?(*.)+(spec|test).[jt]s?(x)?(.snap)",
      "!{projectRoot}/tsconfig.spec.json",
      "!{projectRoot}/jest.config.[jt]s",
      "!{projectRoot}/**/*.stories.@(js|jsx|ts|tsx|mdx)",
      "!{projectRoot}/.storybook/**/*"
    ],
    "production": [
      "default",
      "!{projectRoot}/**/?(*.)+(spec|test).[jt]s?(x)?(.snap)",
      "!{projectRoot}/tsconfig.spec.json",
      "!{projectRoot}/jest.config.[jt]s",
      "!{projectRoot}/src/test-setup.[jt]s",
      "!{projectRoot}/test-setup.[jt]s",
      "!{projectRoot}/**/*.stories.@(js|jsx|ts|tsx|mdx)",
      "!{projectRoot}/.storybook/**/*",
      "!{projectRoot}/tsconfig.storybook.json"
    ]
  },
  "release": {
    "version": {
      "preVersionCommand": "pnpm dlx nx run-many -t build"
    }
  }
}
