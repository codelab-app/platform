{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "rootDirs": ["*", "libs"],
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "strict": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "useDefineForClassFields": true,
    "resolveJsonModule": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "types": [
      "node",
      "worker-loader",
      "window",
      "@emotion/react/types/css-prop"
    ],
    "typeRoots": ["node_modules/@types", "types", "types/*"],
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@codelab/backend": ["libs/backend/src/index.ts"],
      "@codelab/frontend/abstract/core": [
        "libs/frontend/abstract/core/src/index.ts"
      ],
      "@codelab/frontend/abstract/types": [
        "libs/frontend/abstract/types/src/index.ts"
      ],
      "@codelab/frontend/model/infra/graphql": [
        "libs/frontend/model/infra/graphql/src/index.ts"
      ],
      "@codelab/frontend/model/infra/mobx": [
        "libs/frontend/model/infra/mobx/src/index.ts"
      ],
      "@codelab/frontend/modules/admin": [
        "libs/frontend/modules/admin/src/index.ts"
      ],
      "@codelab/frontend/modules/app": [
        "libs/frontend/modules/app/src/index.ts"
      ],
      "@codelab/frontend/modules/atom": [
        "libs/frontend/modules/atom/src/index.ts"
      ],
      "@codelab/frontend/modules/builder": [
        "libs/frontend/modules/builder/src/index.ts"
      ],
      "@codelab/frontend/modules/component": [
        "libs/frontend/modules/component/src/index.ts"
      ],
      "@codelab/frontend/modules/element": [
        "libs/frontend/modules/element/src/index.ts"
      ],
      "@codelab/frontend/modules/lambda": [
        "libs/frontend/modules/lambda/src/index.ts"
      ],
      "@codelab/frontend/modules/page": [
        "libs/frontend/modules/page/src/index.ts"
      ],
      "@codelab/frontend/modules/prop": [
        "libs/frontend/modules/prop/src/index.ts"
      ],
      "@codelab/frontend/modules/props": [
        "libs/frontend/modules/prop/src/index.ts"
      ],
      "@codelab/frontend/modules/store": [
        "libs/frontend/modules/store/src/index.ts"
      ],
      "@codelab/frontend/modules/style": [
        "libs/frontend/modules/style/src/index.ts"
      ],
      "@codelab/frontend/modules/tag": [
        "libs/frontend/modules/tag/src/index.ts"
      ],
      "@codelab/frontend/modules/type": [
        "libs/frontend/modules/type/src/index.ts"
      ],
      "@codelab/frontend/modules/user": [
        "libs/frontend/modules/user/src/index.ts"
      ],
      "@codelab/frontend/presenter/container": [
        "libs/frontend/presenter/container/src/index.ts"
      ],
      "@codelab/frontend/shared/utils": [
        "libs/frontend/shared/utils/src/index.ts"
      ],
      "@codelab/frontend/view/components": [
        "libs/frontend/view/components/index.ts"
      ],
      "@codelab/frontend/view/sections": [
        "libs/frontend/view/sections/index.ts"
      ],
      "@codelab/frontend/view/style": ["libs/frontend/view/style/index.ts"],
      "@codelab/frontend/view/templates": [
        "libs/frontend/view/templates/index.ts"
      ],
      "@codelab/rtk-query": ["libs/tools/rtk-query/src/index.ts"],
      "@codelab/shared/abstract/codegen": [
        "libs/shared/abstract/codegen/src/index.ts"
      ],
      "@codelab/shared/abstract/core": [
        "libs/shared/abstract/core/src/index.ts"
      ],
      "@codelab/shared/abstract/types": [
        "libs/shared/abstract/types/src/index.ts"
      ],
      "@codelab/shared/utils": ["libs/shared/utils/src/index.ts"],
      "@codelab/tools/plugins/codelab": [
        "libs/tools/plugins/codelab/src/index.ts"
      ],
      "@codelab/ui/d3": ["libs/ui/d3/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp", "dist"]
}
