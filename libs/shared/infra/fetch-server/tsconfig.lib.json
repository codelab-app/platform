{
  "extends": "../../../../tsconfig.build.base.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "outDir": "../../../../dist/out-tsc",
    "rootDir": "./src",
    "importHelpers": true,
    "types": []
  },
  "exclude": [
    "jest.config.ts",
    "src/**/*.spec.ts",
    "src/**/*.test.ts",
    "src/**/*.spec.tsx",
    "src/**/*.test.tsx",
    "**/*.stories.ts",
    "**/*.stories.js",
    "**/*.stories.jsx",
    "**/*.stories.tsx"
  ],
  "include": ["src/**/*.ts", "src/**/*.tsx"],
  "references": [
    {
      "path": "../auth0/tsconfig.lib.json"
    },
    {
      "path": "../logging/tsconfig.lib.json"
    },
    {
      "path": "../../config/env/tsconfig.lib.json"
    },
    {
      "path": "../../abstract/types/tsconfig.lib.json"
    }
  ]
}
