{
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "name": "shared-data-seed",
  "projectType": "library",
  "sourceRoot": "libs/shared/data/seed/src",
  "tags": ["projectType:library", "type:concrete", "scope:shared"],
  "targets": {
    "lint": {
      "configurations": {
        "ci": {
          "format": "junit",
          "outputFile": "tmp/reports/lint/shared-data-seed.xml",
          "quiet": true
        }
      },
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": [
          "libs/shared/data/seed/**/*.{ts,tsx,js,jsx,graphql}"
        ]
      },
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      },
      "executor": "@nx/jest:jest",
      "options": {
        "jestConfig": "libs/shared/data/seed/jest.config.ts",
        "reporters": ["default"]
      }
    },
    "test:integration": {
      "configurations": {
        "ci": {
          "parallel": 3
        },
        "dev": {
          "reporters": ["default"]
        },
        "test": {
          "reporters": ["default"]
        }
      },
      "defaultConfiguration": "dev",
      "defaultConfiguration": "dev",
      "executor": "@nx/jest:jest",
      "options": {
        "color": true,
        "jestConfig": "libs/shared/data/seed/jest.config.ts",
        "memoryLimit": 8192,
        "testPathPattern": ["[i].spec.ts"]
      },
      "options": {
        "color": true,
        "jestConfig": "libs/shared/data/seed/jest.config.ts",
        "memoryLimit": 8192,
        "passWithNoTests": true,
        "testPathPattern": ["[i].spec.ts"]
      },
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"]
    },
    "test:unit": {
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        },
        "dev": {
          "reporters": ["default"]
        },
        "test": {
          "reporters": ["default"]
        }
      },
      "defaultConfiguration": "dev",
      "defaultConfiguration": "dev",
      "executor": "@nx/jest:jest",
      "options": {
        "color": true,
        "jestConfig": "libs/shared/data/seed/jest.config.ts",
        "memoryLimit": 8192,
        "parallel": 3,
        "testPathPattern": ["[^i].spec.ts"]
      },
      "options": {
        "color": true,
        "jestConfig": "libs/shared/data/seed/jest.config.ts",
        "memoryLimit": 8192,
        "parallel": 3,
        "passWithNoTests": true,
        "testPathPattern": ["[^i].spec.ts"]
      },
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"]
    }
  }
}
