{
  "root": "libs/backend/modules/app",
  "sourceRoot": "libs/backend/modules/app/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/backend/modules/app/**/*.ts"]
      },
      "outputs": ["{options.outputFile}"],
      "configurations": {
        "ci": {
          "eslintConfig": "libs/backend/modules/app/.eslintrc.ci.json"
        }
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/backend/modules/app"],
      "options": {
        "jestConfig": "libs/backend/modules/app/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["layer:domain:backend", "scope:backend"]
}
