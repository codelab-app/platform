{
  "name": "backend-application-shared",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/backend/application/shared/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/backend/application/shared/**/*.ts"]
      },
      "configurations": {
        "ci": {
          "format": "junit",
          "outputFile": "tmp/reports/lint/backend-application-service.xml",
          "quiet": true
        }
      }
    }
  },
  "tags": ["scope:backend"]
}
