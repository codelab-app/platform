{
  "extends": "../../../../tsconfig.build.base.json",
  "compilerOptions": {
    "outDir": "./dist",
    "declaration": true,
    "types": ["node"],
    "target": "es2021",
    "strictNullChecks": true,
    "noImplicitAny": true,
    "strictBindCallApply": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "rootDir": "./src",
    "tsBuildInfoFile": "./dist/tsconfig.tsbuildinfo"
  },
  "include": ["src/**/*.ts"],
  "exclude": ["jest.config.ts", "src/**/*.spec.ts", "src/**/*.test.ts"],
  "references": [
    {
      "path": "../../../shared/infra/typebox/tsconfig.lib.json"
    },
    {
      "path": "../../../shared/infra/gqlgen/tsconfig.lib.json"
    },
    {
      "path": "../../../shared/abstract/core/tsconfig.lib.json"
    },
    {
      "path": "../../domain/type/tsconfig.lib.json"
    },
    {
      "path": "../../domain/store/tsconfig.lib.json"
    },
    {
      "path": "../../domain/shared/modules/tsconfig.lib.json"
    },
    {
      "path": "../../domain/resource/tsconfig.lib.json"
    },
    {
      "path": "../../domain/prop/tsconfig.lib.json"
    },
    {
      "path": "../../domain/action/tsconfig.lib.json"
    },
    {
      "path": "../type/tsconfig.lib.json"
    }
  ]
}
