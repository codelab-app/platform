    },
    {
      "path": "../../../shared/infra/gql-client/tsconfig.lib.json"
    },
    {
      "path": "../../infra/adapter/logger/tsconfig.lib.json"
    },
    {
      "path": "../../../shared/domain/module/domain/tsconfig.lib.json"
    },
    {
      "path": "../../infra/adapter/dns/tsconfig.lib.json"
