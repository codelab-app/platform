{
  "sourceRoot": "libs/backend/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/backend/**/*.ts"]
      }
    },
    "generate-ogm": {
      "executor": "@nrwl/workspace:run-commands",
      "configurations": {
        "my-script": {
          "commands": ["ts-node libs/backend/src/infra/generate-ogm-types.ts"]
        }
      }
    }
  },
  "tags": ["scope:frontend"]
}
