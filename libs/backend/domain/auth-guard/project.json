{
  "name": "backend-domain-auth-guard",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/backend/domain/auth-guard/src",
  "tags": [
    "projectType:library",
    "type:concrete",
    "scope:backend",
    "layer:domain"
  ],
  "targets": {
    "lint": {
      "configurations": {
        "ci": {
          "format": "junit",
          "outputFile": "tmp/reports/lint/backend-domain-auth-guard.xml",
          "quiet": true
        }
      },
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/backend/domain/auth-guard/**/*.{ts,tsx,js,jsx,graphql}"
        ]
      },
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      },
      "executor": "@nx/jest:jest",
      "options": {
        "jestConfig": "libs/backend/domain/auth-guard/jest.config.ts",
        "passWithNoTests": true,
        "reporters": ["default"]
      },
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"]
    },
    "test:integration": {
      "defaultConfiguration": "dev",
      "options": {
        "memoryLimit": 8192,
        "color": true,
        "testPathPattern": ["[i].spec.ts"],
        "jestConfig": "libs/backend/domain/auth-guard/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "dev": {
          "reporters": ["default"]
        },
        "test": {
          "reporters": ["default"]
        },
        "ci": {
          "parallel": 3,
          "ci": true,
          "codeCoverage": true
        }
      },
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"]
    },
    "test:unit": {
      "defaultConfiguration": "dev",
      "options": {
        "color": true,
        "memoryLimit": 8192,
        "parallel": 3,
        "testPathPattern": ["[^i].spec.ts"],
        "jestConfig": "libs/backend/domain/auth-guard/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "dev": {
          "reporters": ["default"]
        },
        "test": {
          "reporters": ["default"]
        },
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      },
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"]
    }
  }
}
