{
  "extends": "../../../tsconfig.build.base.json",
  "compilerOptions": {
    "outDir": "./dist",
    "types": ["node"],
    "resolveJsonModule": true,
    "noEmit": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "customConditions": null
  },
  "include": ["src/**/*.ts", "src/**/*.json", "src/**/*.ts__tmpl__"],
  "exclude": ["jest.config.ts", "src/**/*.spec.ts", "src/**/*.test.ts"]
}
