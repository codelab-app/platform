{
  "$schema": "http://json-schema.org/draft-07/schema",
  "id": "ViteBuildableLibs",
  "title": "Vite Buildable Libraries Generator",
  "type": "object",
  "properties": {
    "projectName": {
      "type": "string",
      "description": "The name of the project.",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "What is the name of the project?"
    },
    "projectRoot": {
      "type": "string",
      "description": "The root directory of the project.",
      "x-prompt": "What is the root directory of the project?"
    }
    // Add other properties corresponding to schema.d.ts
  },
  "required": ["projectName", "projectRoot"]
}
