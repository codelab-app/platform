{
  "name": "@codelab-codegen/typescript-server-fetch",
  "version": "0.0.1",
  "type": "commonjs",
  "description": "GraphQL Code Generator plugin for generating a ready-to-use SDK based on graphql-request and GraphQL operations",
  "peerDependencies": {
    "graphql": "^0.8.0 || ^0.9.0 || ^0.10.0 || ^0.11.0 || ^0.12.0 || ^0.13.0 || ^14.0.0 || ^15.0.0 || ^16.0.0",
    "graphql-request": "^6.0.0",
    "graphql-tag": "^2.0.0"
  },
  "dependencies": {
    "@graphql-codegen/plugin-helpers": "^3.0.0",
    "@graphql-codegen/visitor-plugin-common": "2.13.1",
    "auto-bind": "~4.0.0",
    "tslib": "~2.6.0"
  },
  "devDependencies": {
    "@graphql-codegen/testing": "1.18.0",
    "@graphql-tools/schema": "10.0.4",
    "graphql-request": "6.0.0"
  },
  "exports": {
    ".": {
      "default": "./src/index.ts",
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    }
  },
  "nx": {
    "name": "codelab-codegen-typescript-server-fetch",
    "projectType": "library",
    "sourceRoot": "libs/external/codelab-codegen/typescript-server-fetch/src",
    "tags": [
      "projectName:codelab-codegen-typescript-server-fetch",
      "projectType:library",
      "type:concrete"
    ],
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "release": {
      "version": {
        "generatorOptions": {
          "packageRoot": "dist/{projectRoot}",
          "currentVersionResolver": "git-tag"
        }
      }
    },
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "dist/libs/external/codelab-codegen/typescript-server-fetch",
          "tsConfig": "libs/external/codelab-codegen/typescript-server-fetch/tsconfig.lib.json",
          "packageJson": "libs/external/codelab-codegen/typescript-server-fetch/package.json",
          "main": "libs/external/codelab-codegen/typescript-server-fetch/src/index.ts",
          "assets": [
            "libs/external/codelab-codegen/typescript-server-fetch/*.md"
          ]
        }
      },
      "lint": {
        "executor": "@nx/eslint:lint"
      }
    }
  }
}
