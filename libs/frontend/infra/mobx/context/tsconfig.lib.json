{
  "extends": "../../../../../tsconfig.base.json",
  "compilerOptions": {
    "module": "commonjs",
    "outDir": "./out-tsc/lib",
    "declaration": true,
    "types": ["node"],
    "jsx": "react-jsx",
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true
  },
  "exclude": ["**/*.spec.ts", "**/*.test.ts"],
  "include": ["**/*.ts", "**/*.tsx"],
  "references": []
}
