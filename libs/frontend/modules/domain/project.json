{
  "tags": ["scope:frontend"],
  "sourceRoot": "libs/domain/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/frontend/modules/domain/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/frontend/modules/domain"],
      "options": {
        "jestConfig": "libs/frontend/modules/domain/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
