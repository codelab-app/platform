    },
    {
      "path": "../../abstract/domain/tsconfig.lib.json"
    },
    {
      "path": "../../../shared/domain/module/preference/tsconfig.lib.json"
    },
    {
      "path": "../../../shared/infra/typebox/tsconfig.lib.json"
    },
    {
      "path": "../../../shared/abstract/core/tsconfig.lib.json"
    },
    {
      "path": "../shared/tsconfig.lib.json"
    },
    {
      "path": "../../../shared/infra/gqlgen/tsconfig.lib.json"
    }
  ]
