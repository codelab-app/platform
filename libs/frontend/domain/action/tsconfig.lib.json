{
  "extends": "../../../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "types": [
      "node",
      "@nx/react/typings/cssmodule.d.ts",
      "@nx/react/typings/image.d.ts"
    ],
    "strict": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "jsx": "react-jsx",
    "forceConsistentCasingInFileNames": true,
    "rootDir": "./src"
  },
  "exclude": ["**/*.spec.ts", "**/*.spec.tsx"],
  "include": ["**/*.js", "**/*.jsx", "**/*.ts", "**/*.tsx"],
  "references": [
    {
      "path": "../shared/tsconfig.lib.json"
    },
    {
      "path": "../prop/tsconfig.lib.json"
    },
    {
      "path": "../../../shared/abstract/types/tsconfig.lib.json"
    },
    {
      "path": "../../../shared/domain/module/action/tsconfig.lib.json"
    },
    {
      "path": "../../../shared/infra/gqlgen/tsconfig.lib.json"
    },
    {
      "path": "../../../shared/abstract/core/tsconfig.lib.json"
    },
    {
      "path": "../../abstract/domain/tsconfig.lib.json"
    }
  ]
}
