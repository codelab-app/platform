{
  "extends": "../../../../tsconfig.build.base.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "outDir": "../../../../dist/out-tsc",
    "importHelpers": true,
    "types": [
      "@nx/react/typings/cssmodule.d.ts",
      "@nx/react/typings/image.d.ts"
    ]
  },
  "exclude": [
    "jest.config.ts",
    "src/**/*.spec.ts",
    "src/**/*.test.ts",
    "src/**/*.spec.tsx",
    "src/**/*.test.tsx",
    "**/*.stories.ts",
    "**/*.stories.js",
    "**/*.stories.jsx",
    "**/*.stories.tsx"
  ],
  "include": ["src/**/*.ts", "src/**/*.tsx"],
  "references": []
}
