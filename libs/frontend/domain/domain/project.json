{
  "name": "frontend-domain-domain",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "tags": ["scope:frontend"],
  "sourceRoot": "libs/domain/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/frontend/domain/domain/**/*.{ts,tsx,js,jsx}"]
      },
      "configurations": {
        "ci": {
          "format": "junit",
          "outputFile": "tmp/reports/lint/frontend-domain-domain.xml",
          "quiet": true
        }
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/frontend/domain/domain"],
      "options": {
        "jestConfig": "libs/frontend/domain/domain/jest.config.ts",
        "passWithNoTests": true,
        "reporters": ["default"]
      }
    },
    "test:integration": {
      "options": {
        "memoryLimit": 8192,
        "color": true,
        "testPathPattern": ["[i].spec.ts"],
        "jestConfig": "libs/frontend/domain/domain/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "parallel": 3
        }
      },
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/frontend/domain/domain"]
    },
    "test:unit": {
      "options": {
        "memoryLimit": 8192,
        "parallel": 3,
        "color": true,
        "testPathPattern": ["[^i].spec.ts"],
        "jestConfig": "libs/frontend/domain/domain/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {}
      },
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/frontend/domain/domain"]
    }
  }
}
