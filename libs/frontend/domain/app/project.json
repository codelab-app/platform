{
  "name": "frontend-domain-app",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/frontend/domain/app/src",
  "projectType": "library",
  "tags": [
    "projectName:frontend-domain-app",
    "projectType:library",
    "type:concrete",
    "scope:frontend",
    "layer:domain"
  ],
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["{projectRoot}/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/frontend/domain/app"],
      "options": {
        "jestConfig": "libs/frontend/domain/app/jest.config.ts",
        "runInBand": true
      }
    }
  }
}
