{
  "name": "frontend-domain-tag",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/frontend/domain/tag/src",
  "projectType": "library",
  "tags": [
    "projectName:frontend-domain-tag",
    "projectType:library",
    "type:concrete",
    "scope:frontend",
    "layer:domain"
  ],
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/frontend/domain/tag"],
      "options": {
        "jestConfig": "libs/frontend/domain/tag/jest.config.ts",
        "reporters": ["default"],
        "runInBand": true
      }
    },
    "test:integration": {
      "defaultConfiguration": "dev",
      "options": {
        "memoryLimit": 8192,
        "color": true,
        "testPathPattern": ["i.spec.ts"],
        "runInBand": true,
        "jestConfig": "libs/frontend/domain/tag/jest.config.ts"
      },
      "configurations": {
        "dev": {
          "reporters": ["default"]
        },
        "test": {
          "reporters": ["default"]
        },
        "ci": {
          "parallel": 3
        }
      },
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/frontend/domain/tag"]
    },
    "test:unit": {
      "defaultConfiguration": "dev",
      "options": {
        "color": true,
        "memoryLimit": 8192,
        "parallel": 3,
        "testPathIgnorePatterns": ["i.spec.ts"],
        "jestConfig": "libs/frontend/domain/tag/jest.config.ts"
      },
      "configurations": {
        "dev": {
          "reporters": ["default"]
        },
        "test": {
          "reporters": ["default"]
        },
        "ci": {}
      },
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/frontend/domain/tag"]
    }
  }
}
