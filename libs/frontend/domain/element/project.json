{
  "name": "frontend-domain-element",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/frontend/domain/element/src",
  "tags": [
    "projectName:frontend-domain-element",
    "projectType:library",
    "type:concrete",
    "scope:frontend",
    "layer:domain"
  ],
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "configurations": {
        "dev": {
          "reporters": ["default"]
        },
        "test": {
          "reporters": ["default"]
        }
      },
      "defaultConfiguration": "dev",
      "executor": "@nx/jest:jest",
      "options": {
        "color": true,
        "jestConfig": "libs/frontend/domain/element/jest.config.ts",
        "memoryLimit": 8192,
        "parallel": 3,
        "runInBand": true,
        "testPathPattern": ["[^i].spec.ts"],
        "reporters": ["default"]
      },
      "outputs": ["{workspaceRoot}/coverage/libs/frontend/domain/element"]
    }
  }
}
