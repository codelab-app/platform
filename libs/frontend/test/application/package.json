{
  "name": "@codelab/frontend-test-application",
  "exports": {
    ".": {
      "default": "./src/index.ts",
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    }
  },
  "devDependencies": {
    "@codelab/frontend-abstract-application": "workspace:*",
    "@codelab/frontend-abstract-domain": "workspace:*",
    "@codelab/frontend-shared-utils": "workspace:*",
    "@codelab/frontend-application-builder": "workspace:*",
    "@codelab/frontend-application-dnd": "workspace:*",
    "@codelab/frontend-infra-mobx-store": "workspace:*",
    "@codelab/shared-abstract-core": "workspace:*",
    "@codelab/frontend-test-data": "workspace:*",
    "@codelab/shared-infra-typebox": "workspace:*",
    "@codelab/frontend-application-renderer": "workspace:*"
  },
  "nx": {
    "name": "frontend-test-application",
    "projectType": "library",
    "sourceRoot": "libs/frontend/test/application/src",
    "tags": [
      "projectName:frontend-test-application",
      "projectType:library",
      "type:concrete",
      "type:test",
      "scope:frontend"
