    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "targets": {
      "lint": {
        "executor": "@nx/eslint:lint",
        "outputs": [
          "{options.outputFile}"
        ]
      },
      "test": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{workspaceRoot}/coverage/{projectRoot}"
        ],
        "options": {
          "jestConfig": "libs/frontend/test/application/jest.config.ts",
          "runInBand": true
        }
      }
    }
