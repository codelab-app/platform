{
  "extends": "../../../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./out-tsc/lib",
    "types": [
      "node",
      "../../../../node_modules/@nx/react/typings/cssmodule.d.ts",
      "../../../../node_modules/@nx/react/typings/image.d.ts"
    ],
    "jsx": "react-jsx",
    "allowJs": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
