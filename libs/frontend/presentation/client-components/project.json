{
  "name": "frontend-presentation-client-components",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/frontend/presentation/client-components/src",
  "projectType": "library",
  "tags": [
    "layer:presentation",
    "projectType:library",
    "type:concrete",
    "scope:frontend"
  ],
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint",
      "configurations": {
        "ci": {
          "format": "junit",
          "outputFile": "tmp/reports/lint/frontend-presentation-client-components.xml",
          "quiet": true
        }
      },
      "options": {
        "lintFilePatterns": [
          "libs/frontend/presentation/client-components/**/*.{ts,tsx,js,jsx,graphql}"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/frontend/presentation/client-components/jest.config.ts",
        "reporters": ["default"],
        "runInBand": true
      }
    },
    "test:integration": {
      "defaultConfiguration": "dev",
      "options": {
        "memoryLimit": 8192,
        "color": true,
        "testPathPattern": ["i.spec.ts"],
        "runInBand": true,
        "jestConfig": "libs/frontend/presentation/client-components/jest.config.ts"
      },
      "configurations": {
        "dev": {
          "reporters": ["default"]
        },
        "test": {
          "reporters": ["default"]
        },
        "ci": {
          "parallel": 3
        }
      },
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"]
    },
    "test:unit": {
      "defaultConfiguration": "dev",
      "options": {
        "color": true,
        "memoryLimit": 8192,
        "parallel": 3,
        "testPathIgnorePatterns": ["i.spec.ts"],
        "jestConfig": "libs/frontend/presentation/client-components/jest.config.ts"
      },
      "configurations": {
        "dev": {
          "reporters": ["default"]
        },
        "test": {
          "reporters": ["default"]
        },
        "ci": {}
      },
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"]
    }
  }
}
