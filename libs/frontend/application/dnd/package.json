{
  "name": "@codelab/frontend-application-dnd",
  "exports": {
    ".": {
      "default": "./src/index.ts",
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./collision-detection": {
      "default": "./src/collision-detection/index.ts",
      "import": "./src/collision-detection/index.ts",
      "types": "./src/collision-detection/index.ts"
    },
    "./components": {
      "default": "./src/components/index.ts",
      "import": "./src/components/index.ts",
      "types": "./src/components/index.ts"
    },
    "./hooks": {
      "default": "./src/hooks/index.ts",
      "import": "./src/hooks/index.ts",
      "types": "./src/hooks/index.ts"
    }
  },
  "devDependencies": {
    "@codelab/frontend-shared-utils": "workspace:*",
    "@codelab/shared-abstract-types": "workspace:*"
  },
  "nx": {
    "name": "frontend-application-dnd",
    "projectType": "library",
    "sourceRoot": "libs/frontend/application/dnd/src",
    "tags": [
      "projectName:frontend-application-dnd",
      "projectType:library",
      "type:concrete",
      "scope:frontend",
      "layer:application"
    ],
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "targets": {
      "lint": {
        "executor": "@nx/eslint:lint",
        "outputs": [
          "{options.outputFile}"
        ]
      },
      "test": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{workspaceRoot}/coverage/{projectRoot}"
        ],
        "options": {
          "jestConfig": "libs/frontend/application/dnd/jest.config.ts",
          "runInBand": true
        }
      }
    }
  }
}
