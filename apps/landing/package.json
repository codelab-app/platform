{
  "name": "landing",
  "exports": {
    ".": {
      "default": "./src/index.ts",
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    }
  },
  "devDependencies": {
    "@codelab/frontend-abstract-application": "workspace:*",
    "@codelab/shared-config-env": "workspace:*",
    "@codelab/shared-infra-typebox": "workspace:*",
    "@codelab/shared-config-builder": "workspace:*",
    "@codelab/shared-utils": "workspace:*",
    "@codelab/frontend-presentation-components-fancybox": "workspace:*"
  },
  "nx": {
    "name": "landing",
    "projectType": "application",
    "sourceRoot": "apps/landing",
    "tags": [
      "projectName:landing",
      "projectType:application",
      "type:concrete"
    ],
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "targets": {
      "build": {
        "executor": "@nx/next:build",
        "outputs": [
          "{options.outputPath}"
        ],
        "defaultConfiguration": "prod",
        "options": {
          "outputPath": "dist/apps/landing"
        },
        "configurations": {
          "dev": {
            "outputPath": "apps/landing"
          },
          "ci": {},
          "prod": {}
        }
      },
      "serve": {
        "executor": "@nx/next:server",
        "defaultConfiguration": "dev",
        "options": {
          "buildTarget": "landing:build",
          "dev": true
        },
        "configurations": {
          "dev": {
            "buildTarget": "landing:build:dev",
            "dev": true
          },
          "ci": {
            "buildTarget": "landing:build:ci",
            "dev": false
          },
          "prod": {
            "buildTarget": "landing:build:prod",
            "dev": false
          }
        }
      },
      "export": {
        "executor": "@nx/next:export",
        "options": {
          "buildTarget": "landing:build:prod"
        }
      },
      "lint": {
        "executor": "@nx/eslint:lint"
      }
    }
  }
}
