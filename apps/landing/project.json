{
  "name": "landing",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/landing",
  "projectType": "application",
  "tags": ["projectName:landing", "projectType:application", "type:concrete"],
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "prod",
      "options": {
        "outputPath": "dist/apps/landing"
      },
      "inputs": [
        {
          "env": "NEXT_PUBLIC_GOOGLE_ANALYTICS"
        },
        {
          "env": "NEXT_PUBLIC_HOTJAR_ID"
        },
        {
          "env": "NEXT_PUBLIC_HOTJAR_VERSION"
        },
        {
          "env": "NEXT_PUBLIC_INTERCOM_APP_ID"
        },
        {
          "env": "NEXT_PUBLIC_SUPABASE_KEY"
        },
        {
          "env": "NEXT_PUBLIC_SUPABASE_URL"
        }
      ],
      "configurations": {
        "dev": {
          "outputPath": "apps/landing"
        },
        "ci": {},
        "prod": {}
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "dev",
      "options": {
        "buildTarget": "landing:build",
        "dev": true
      },
      "configurations": {
        "dev": {
          "buildTarget": "landing:build:dev",
          "dev": true
        },
        "ci": {
          "buildTarget": "landing:build:ci",
          "dev": false
        },
        "prod": {
          "buildTarget": "landing:build:prod",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "landing:build:prod"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
