{
  "presets": [
    [
      "@nrwl/next/babel",
      {
        "preset-react": {
          "runtime": "automatic",
          "importSource": "@emotion/react"
        }
      }
    ],
    // Need to to use `css` property for tailwind
    "@emotion/babel-preset-css-prop"
  ],
  "plugins": ["@emotion", "macros"],
  "env": {
    "development": {
      "presets": [
        [
          "@nrwl/next/babel",
          {
            "preset-react": {
              "runtime": "automatic",
              "development": true,
              "importSource": "@welldone-software/why-did-you-render"
            }
          }
        ]
      ]
    }
  }
}
