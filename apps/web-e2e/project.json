{
  "name": "web-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/web-e2e/src",
  "tags": ["projectName:web-e2e", "projectType:application", "type:concrete"],
  "implicitDependencies": ["web"],
  "targets": {
    "e2e": {
      "defaultConfiguration": "test",
      "executor": "@nx/playwright:playwright",
      "outputs": ["{workspaceRoot}/dist/.playwright/apps/web-e2e"],
      "options": {
        "config": "apps/web-e2e/playwright.config.ts",
        "fullyParallel": false,
        "workers": 1,
        "headed": false,
        "ui": false,
        "testFiles": [
          "pages.spec.ts",
          "resource.spec.ts",
          "types.spec.ts",
          "google-fonts.spec.ts",
          "provider-page-content-container.spec.ts"
        ]
      },
      "configurations": {
        "test": {},
        "ci": {}
      }
    },
    "e2e-ui": {
      "defaultConfiguration": "test",
      "executor": "@nx/playwright:playwright",
      "outputs": ["{workspaceRoot}/dist/.playwright/apps/web-e2e"],
      "options": {
        "config": "apps/web-e2e/playwright.config.ts",
        "fullyParallel": false,
        "workers": 1,
        "headed": true,
        "ui": true
      },
      "configurations": {
        "test": {
          "fullyParallel": false,
          "workers": 1
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
