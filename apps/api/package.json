{
  "name": "api",
  "exports": {
    ".": {
      "default": "./src/index.ts",
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    }
  },
  "devDependencies": {
    "@codelab/backend-application-action": "workspace:*",
    "@codelab/backend-application-admin": "workspace:*",
    "@codelab/backend-application-app": "workspace:*",
    "@codelab/backend-application-atom": "workspace:*",
    "@codelab/backend-application-component": "workspace:*",
    "@codelab/backend-application-domain": "workspace:*",
    "@codelab/backend-application-element": "workspace:*",
    "@codelab/backend-application-field": "workspace:*",
    "@codelab/backend-application-redirect": "workspace:*",
    "@codelab/backend-application-resource": "workspace:*",
    "@codelab/backend-application-tag": "workspace:*",
    "@codelab/backend-application-user": "workspace:*",
    "@codelab/backend-infra-adapter-request-context": "workspace:*",
    "@codelab/backend-infra-core": "workspace:*",
    "@codelab/backend-infra-adapter-graphql": "workspace:*",
    "@codelab/backend-infra-adapter-logger": "workspace:*",
    "@codelab/backend-application-auth": "workspace:*",
    "@codelab/backend-domain-shared-modules": "workspace:*",
    "@codelab/backend-infra-adapter-queue": "workspace:*",
    "@codelab/backend-infra-adapter-ws": "workspace:*",
    "@codelab/backend-infra-adapter-neo4j-schema": "workspace:*"
  },
  "nx": {
    "name": "api",
    "projectType": "application",
    "sourceRoot": "apps/api/src",
    "tags": [
      "projectName:api",
      "projectType:application",
      "type:concrete"
    ],
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "targets": {
      "build": {
        "defaultConfiguration": "prod",
        "executor": "@nx/webpack:webpack",
        "outputs": [
          "{options.outputPath}"
        ],
        "configurations": {
          "ci": {
            "optimization": true
          },
          "dev": {},
          "prod": {
            "optimization": true
          },
          "test": {
            "outputPath": "dist/apps/api-test"
          }
        },
        "options": {
          "compiler": "tsc",
          "main": "apps/api/src/main.ts",
          "outputPath": "dist/apps/api",
          "target": "node",
          "tsConfig": "apps/api/tsconfig.json",
          "webpackConfig": "apps/api/webpack.config.js"
        }
      },
      "lint": {
        "executor": "@nx/eslint:lint"
      },
      "serve": {
        "defaultConfiguration": "dev",
        "executor": "@nx/js:node",
        "configurations": {
          "ci": {
            "buildTarget": "api:build:ci"
          },
          "dev": {},
          "prod": {
            "buildTarget": "api:build:prod"
          },
          "test": {
            "buildTarget": "api:build:test"
          }
        },
        "options": {
          "buildTarget": "api:build:dev"
        }
      },
      "test": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{workspaceRoot}/coverage/{projectRoot}"
        ],
        "options": {
          "jestConfig": "apps/api/jest.config.ts",
          "runInBand": true
        }
      }
    }
  }
}
