{
  "name": "web",
  "devDependencies": {
    "@codelab/frontend-infra-context": "workspace:*",
    "@codelab/frontend-test-data": "workspace:*",
    "@codelab/frontend-infra-mobx-store": "workspace:*",
    "@codelab/shared-abstract-core": "workspace:*",
    "@codelab/frontend-application-admin": "workspace:*",
    "@codelab/frontend-presentation-view": "workspace:*",
    "@codelab/frontend-application-user": "workspace:*",
    "@codelab/frontend-domain-user": "workspace:*",
    "@codelab/frontend-application-app": "workspace:*",
    "@codelab/frontend-abstract-types": "workspace:*",
    "@codelab/frontend-application-shared-store": "workspace:*",
    "@codelab/frontend-presentation-codelab-ui": "workspace:*",
    "@codelab/frontend-application-domain": "workspace:*",
    "@codelab/frontend-domain-domain": "workspace:*",
    "@codelab/frontend-abstract-application": "workspace:*",
    "@codelab/frontend-application-builder": "workspace:*",
    "@codelab/frontend-application-store": "workspace:*",
    "@codelab/frontend-application-element": "workspace:*",
    "@codelab/frontend-application-type": "workspace:*",
    "@codelab/frontend-application-page": "workspace:*",
    "@codelab/frontend-application-redirect": "workspace:*",
    "@codelab/frontend-application-atom": "workspace:*",
    "@codelab/frontend-domain-atom": "workspace:*",
    "@codelab/frontend-application-auth-guard": "workspace:*",
    "@codelab/frontend-domain-auth-guard": "workspace:*",
    "@codelab/frontend-application-component": "workspace:*",
    "@codelab/frontend-application-lambda": "workspace:*",
    "@codelab/frontend-domain-preference": "workspace:*",
    "@codelab/frontend-application-resource": "workspace:*",
    "@codelab/frontend-domain-resource": "workspace:*",
    "@codelab/frontend-application-tag": "workspace:*",
    "@codelab/frontend-domain-tag": "workspace:*",
    "@codelab/frontend-domain-type": "workspace:*",
    "@codelab/backend-infra-adapter-middleware": "workspace:*",
    "@codelab/shared-infra-logging": "workspace:*",
    "@codelab/shared-domain-module-domain": "workspace:*",
    "@codelab/shared-infra-gql-client": "workspace:*",
    "@codelab/shared-infra-auth0": "workspace:*",
    "@cui/utils": "workspace:*"
  },
  "nx": {
    "name": "web",
    "projectType": "application",
    "sourceRoot": "apps/web",
    "tags": [
      "projectName:web",
      "projectType:application",
      "type:concrete"
    ],
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "implicitDependencies": [],
    "generators": {},
    "targets": {
      "build": {
        "defaultConfiguration": "dev",
        "executor": "@nx/next:build",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "dist/apps/web",
          "postcssConfig": "apps/web/postcss.config.js"
        },
        "configurations": {
          "dev": {},
          "ci": {},
          "test": {
            "outputPath": "dist/apps/web-test"
          },
          "prod": {}
        }
      },
      "serve": {
        "defaultConfiguration": "dev",
        "executor": "@nx/next:server",
        "options": {
          "buildTarget": "web:build",
          "port": 3000,
          "turbo": true,
          "hostname": "0.0.0.0",
          "maxWorkers": 4
        },
        "configurations": {
          "dev": {
            "buildTarget": "web:build:dev"
          },
          "local": {
            "buildTarget": "web:build:dev",
            "dev": false
          },
          "test": {
            "buildTarget": "web:build:test",
            "port": 3001
          },
          "ci": {
            "buildTarget": "web:build:ci",
            "dev": false
          },
          "prod": {
            "buildTarget": "web:build:prod",
            "dev": false
          }
        }
      },
      "export": {
        "executor": "@nx/next:export",
        "options": {
          "buildTarget": "web:build:prod"
        }
      },
      "lint": {
        "executor": "@nx/eslint:lint"
      },
      "test": {
        "executor": "@nx/jest:jest",
        "options": {
          "jestConfig": "apps/web/jest.config.ts",
          "runInBand": true
        },
        "configurations": {
          "test": {
            "testPathPattern": [
              "i.spec.ts"
            ],
            "runInBand": true
          },
          "ci": {
            "testPathPattern": [
              "i.spec.ts"
            ],
            "runInBand": true
          }
        },
        "outputs": [
          "{workspaceRoot}/coverage/apps/web"
        ]
      },
      "storybook": {
        "executor": "@nx/storybook:storybook",
        "options": {
          "port": 4400,
          "configDir": "apps/web/.storybook"
        },
        "configurations": {
          "ci": {
            "quiet": true
          }
        }
      },
      "build-storybook": {
        "executor": "@nx/storybook:build",
        "outputs": [
          "{options.outputDir}"
        ],
        "options": {
          "outputDir": "dist/storybook/web",
          "configDir": "apps/web/.storybook"
        },
        "configurations": {
          "ci": {
            "quiet": true
          }
        }
      },
      "test-storybook": {
        "executor": "nx:run-commands",
        "options": {
          "command": "test-storybook -c apps/web/.storybook --url=http://localhost:4400"
        }
      },
      "static-storybook": {
        "executor": "@nx/web:file-server",
        "dependsOn": [
          "build-storybook"
        ],
        "options": {
          "buildTarget": "web:build-storybook",
          "staticFilePath": "dist/storybook/web",
          "spa": true
        },
        "configurations": {
          "ci": {
            "buildTarget": "web:build-storybook:ci"
          }
        }
      },
      "build-profile": {
        "executor": "nx:run-commands",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "command": "export NODE_OPTIONS='--heap-prof' && nx run web:build:{args.configuration}",
          "outputPath": "dist/apps/web"
        },
        "configurations": {
          "dev": {
            "command": "export NODE_OPTIONS='--heap-prof' && nx run web:build:dev"
          },
          "ci": {
            "command": "export NODE_OPTIONS='--heap-prof' && nx run web:build:ci"
          },
          "test": {
            "command": "export NODE_OPTIONS='--heap-prof' && nx run web:build:test",
            "outputPath": "dist/apps/web-test"
          },
          "prod": {
            "command": "export NODE_OPTIONS='--heap-prof' && nx run web:build:prod"
          }
        }
      }
    }
  }
}
