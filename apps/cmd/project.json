{
  "root": "apps/cmd",
  "sourceRoot": "apps/cmd/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/cmd",
        "main": "apps/cmd/src/main.ts",
        "tsConfig": "apps/cmd/tsconfig.app.json",
        "assets": ["apps/cmd/src/assets"]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/cmd/src/environments/environment.ts",
              "with": "apps/cmd/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:execute",
      "options": {
        "buildTarget": "cmd:build",
        "inspect": true,
        "port": 9229
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/cmd/**/*.ts"]
      },
      "outputs": ["{options.outputFile}"],
      "configurations": {
        "ci": {
          "eslintConfig": "apps/cmd/.eslintrc.ci.json"
        }
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/cmd"],
      "options": {
        "jestConfig": "apps/cmd/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["scope:backend"]
}
