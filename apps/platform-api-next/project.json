{
  "name": "platform-api-next",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/platform-api-next",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "prod",
      "options": {
        "outputPath": "dist/apps/platform-api-next"
      },
      "configurations": {
        "dev": {
          "outputPath": "apps/platform-api-next"
        },
        "prod": {}
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "dev",
      "options": {
        "buildTarget": "platform-api-next:build",
        "dev": true
      },
      "configurations": {
        "dev": {
          "buildTarget": "platform-api-next:build:dev",
          "dev": true
        },
        "prod": {
          "buildTarget": "platform-api-next:build:prod",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "platform-api-next:build:prod"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/platform-api-next/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": ["scope:frontend"]
}
