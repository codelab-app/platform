                "command": "node dist/apps/api/main.js"
              },
              {
                "command": "npx wait-on 'http://127.0.0.1:3001' 'http://127.0.0.1:3334' && echo 'READY'"
              }
            ],
            "parallel": true,
            "readyWhen": "READY"
          }
        },
        "serve": {
          "builder": "@nrwl/next:server",
          "options": {
            "buildTarget": "web:build",
            "dev": true,
            "port": 3000,
            "maxWorkers": 4,
            "memoryLimit": 2048
          },
          "configurations": {
            "ci": {
              "memoryLimit": 4096
            },
            "local": {
              "port": 3001
            },
            "dev": {},
            "production": {
              "buildTarget": "web:build-next:production",
              "dev": false
            }
          }
        },
        "export": {
          "builder": "@nrwl/next:export",
          "options": {
            "buildTarget": "web:build:production"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/web/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/web/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/apps/web"]
        },
        "storybook": {
          "builder": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "apps/web/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "builder": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/web",
            "config": {
              "configFolder": "apps/web/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "web-e2e": {
      "root": "apps/web-e2e",
      "sourceRoot": "apps/web-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/web-e2e/cypress.json",
            "tsConfig": "apps/web-e2e/tsconfig.e2e.json"
          },
          "configurations": {
            "ci": {
              "devServerTarget": "web:serve-with-api",
              "baseUrl": "http://127.0.0.1:3001",
              "headless": true
            },
            "local": {
              "devServerTarget": "web:serve-with-api",
              "baseUrl": "http://127.0.0.1:3001",
              "headless": true
            },
            "dev": {
              "baseUrl": "http://127.0.0.1:3000",
              "watch": true,
              "exit": false
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/web-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  }
}
